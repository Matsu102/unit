<h2>Users ユーザ一覧</h2>

<% @users.each do |user| %>
  <%= link_to admin_user_path(user.id) do %>
    <div>
      <%= image_tag user.get_thumbnail(50,50) %>
      </p>
        会員ID <%= user.id %><br>
        <%= user.handle_name %><br>
        <%= user.last_name %> <%= user.first_name %>
    </div>
  <% end %>
<% end %>
<h2>Inquiries 問い合わせ一覧</h2>

<table>
  <tr>
    <th>ステータス</th>
    <th>問い合わせ日時</th>
    <th>会員ID</th>
    <th>名前</th>
    <th>件名</th>
  </tr>
  <tr>
  <% @inquiries.each do |inquiry| %>
    <td><%= inquiry.is_response ? "未対応" : "対応済み" %> %></td>
    <td><%= inquiry.created_at %></td>
    <td><%= inquiry.member_id %></td>
    <td><%= inquiry.last_name %> <%= inquiry.first_name %></td>
    <td><%= inquiry.inquiry_name %></td>
  <% end %>
  </tr>
</table>